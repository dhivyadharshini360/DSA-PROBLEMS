class Solution {

    void printTriangle(int n) {
        // code here
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print("*"+" ");
            }
            System.out.println();
        }
        
        for(int k=1;k<=n-1;k++){
            for(int l=1;l<=n-1-k+1;l++){
                System.out.print("*"+" ");
            }
            System.out.println();
        }
        
    }
}
