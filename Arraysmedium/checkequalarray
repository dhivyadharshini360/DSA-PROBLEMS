class Solution {
    public static boolean checkEqual(int[] a, int[] b) {
    Arrays.sort(a);
    Arrays.sort(b);
    int count=0;
    for(int i=0;i<a.length;i++){
        if(a[i]==b[i]){
            count++;
        }
        else{
            return false;
        }
    }
    return true;
    }
}
