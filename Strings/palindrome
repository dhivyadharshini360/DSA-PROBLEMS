class Solution {
    boolean isPalindrome(String s) {
        // code here
        int i=0;
        int j=s.length()-1;
        char[] ss=s.toCharArray();
        while(i<=j){
            if(ss[i]!=ss[j]){
                return false;
            }
            i++;
            j--;
        }
        return true;
    }
}
